  <!-- Language (INT+CHA) Skill -->
  <thing
    id="skLang"
    name="Language"
    compset="Skill"
    isunique="no"
    description=" ">
    <fieldval field="trtAbbrev" value="Lang"/>

    <!-- If this skill should be shown as an out-of-combat skill on the TacCon, assign this tag    -->
    <tag group="DashTacCon" tag="NonCombat"/>
    <tag group="SkillType" tag="Prof"/>
    <tag group="Helper" tag="CustomItem"/>

	<!-- Tag the cultures and professions for which this skill is legal -->
    <tag group="SkillSrc" tag="Civilised"/>
	
    <!-- Specify the Charact to which this skill is linked -->
    <link linkage="Char1" thing="attrInt"/>
    <link linkage="Char2" thing="attrCha"/>
	
    <!-- Calculate the skill as appropriate -->
    <eval value="1" phase="Traits" priority="4000">
      <before name="Derived trtFinal"/> 
      <after name="Calc trtFinal"/><![CDATA[ 
      var str as string
	  str = left(field[UserName].text,11)
	  if (compare(str,"Language") + field[UserName].isempty = 0) then
	      trustme
          field[UserName].text = "Language: " & field[UserName].text
		  endif
      ]]></eval> 
    </thing>
		
		
		
